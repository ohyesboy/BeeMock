<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BeeMock.TabbedPage1"
             xmlns:local="clr-namespace:BeeMock"
            xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
            android:TabbedPage.ToolbarPlacement="Bottom"
             Title="TabbedPage1">
    <local:MainPage Title="Library">
        <ContentPage.IconImageSource>
            <FontImageSource
                FontFamily="icons"
                Color="{StaticResource Primary}"
                Glyph="{Static local:Icons.Home}"/>
        </ContentPage.IconImageSource>
    </local:MainPage>
    <local:VocabPage Title="Vocabs" >
                <ContentPage.IconImageSource>
            <FontImageSource
                
                FontFamily="icons"
                Color="Black"
                Glyph="{Static local:Icons.Doc_Text}"/>
        </ContentPage.IconImageSource>
    </local:VocabPage>
    <local:VocabPage Title="Bees" />
    <local:VocabPage Title="More" />
</TabbedPage>
